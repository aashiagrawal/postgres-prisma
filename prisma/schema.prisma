generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Chart {
  id          Int       @id @default(autoincrement())
  name        String
  dashboardId Int
  sqlQuery    String
  xAxisField  String
  yAxisField  String
  Dashboard   Dashboard @relation(fields: [dashboardId], references: [id])
}

model Dashboard {
  id    Int     @id @default(autoincrement())
  name  String  @unique
  Chart Chart[]
}
